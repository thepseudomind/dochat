}},{"source":"../src/components/ListTile/ListTile.scss","name":null,"original":{"line":61,"column":45},"generated":{"line":49,"column":56}},{"source":"../src/components/ListTile/ListTile.scss","name":null,"original":{"line":62,"column":5},"generated":{"line":49,"column":59}}],"sources":{"../src/styles/abstract/_mixins.scss":"//MEDIA QUERIES\n/* \n    Phones = 0px-600px,\n    Tablet(Portrait) = 600px-900px,\n    Tablet(Landscape) = 900px-1200px\n    Desktop = 1200px-1800px (Regular styles apply)\n    Big Desktop = >1800px \n*/\n@mixin respond($device) {\n    @if($device == phone){\n        @media (max-width: 37.5em) {\n            @content;\n        }\n    }\n\n    @if($device == tab-port){\n        @media (max-width: 56.25em) {\n            @content;\n        }\n    }\n\n    @if($device == tab-land){\n        @media (max-width: 75em) {\n            @content;\n        }\n    }\n\n    @if($device == big-desk){\n        @media (min-width: 112.5em) {\n            @content;\n        }\n    }\n}","../src/components/ListTile/ListTile.scss":"@import '../../styles/abstract/mixins';\n@import '../../styles/abstract/variables';\n\n.list-tile{\n    width: 100%;\n    margin: 0 auto;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    padding: 1.25rem 0;\n    cursor: pointer;\n    border-bottom: .1rem solid rgba(black, .2); //to be removed if there is more than one chat room\n    \n    &:not(:last-child){\n        border-bottom: .1rem solid rgba(black, .2);\n    }\n\n    @include respond(tab-port){\n        width: 100%;\n        padding: 1.5rem;\n        padding-left: 2.5rem;\n        color: rgba(black, .7);\n    }\n\n    &__text{\n        width: 70%;\n        display: flex;\n        flex-direction: column;\n        justify-content: space-between;\n\n        & > *{\n            padding-left: 1rem;\n        }\n\n        &--title{\n            font-size: 1.5rem;\n            font-weight: 700;\n        }\n\n        &--comment{\n            display: inline-block;\n            font-size: 1.2rem;\n        }\n\n        @include respond(tab-port){\n            width: 80%;\n        }\n\n        @include respond(phone){\n            width: 65%;\n        }\n    }\n\n    &__time{\n        font-size: 1rem;\n    }\n\n    &.dark{\n        color: white;\n        background-color: transparent;\n        border-bottom: .1rem solid rgba(white, .2);\n    }\n}"},"lineCount":null}},"error":null,"hash":"291b0b4a00262b919952cf97938fd59a","cacheData":{"env":{}}}